<?xml version="1.0" encoding="UTF-8"?>
<project name="prepare-visminer-web" default="import-visminer-core-jar" basedir=".">
	<target name="import-visminer-core-jar">
	    
		<javac srcdir="../Visminer/src/main/java" destdir="../Visminer/build/classes">
			<classpath>
				<path path="${javac.classpath}" />
			</classpath>
		</javac>
		<jar destfile="${project_path}/web/WEB-INF/Visminer/Visminer.jar" basedir="${workspace}/../Visminer/build/classes"
			includes="*.class" />
	</target>
</project>